{
  "name": "nextjs-builder-io-landing-page",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prettier": "prettier"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages",
      "sections"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@analytics/google-tag-manager": "^0.5.2",
    "@builder.io/app-context": "^1.0.0",
    "@builder.io/react": "^2.0.10",
    "@builder.io/widgets": "^1.2.23",
    "@vercel/fetch": "^6.1.0",
    "analytics": "^0.8.1",
    "atob": "^2.1.2",
    "jest": "^26.6.3",
    "latest": "^0.2.0",
    "next": "^13.0.0",
    "next-seo": "^4.11.0",
    "node-fetch": "^2.6.7",
    "npm-fetch": "^0.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-merge-refs": "^1.1.0",
    "sass": "^1.53.0"
  },
  "devDependencies": {
    "@emotion/core": "^11.0.0",
    "@next/bundle-analyzer": "^10.0.1",
    "@types/node": "^14.11.2",
    "@types/react": "^18.0.24",
    "bunyan": "^1.8.14",
    "bunyan-prettystream": "^0.1.3",
    "next-unused": "^0.0.3",
    "prettier": "^2.1.2",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "webpack": "^5.0.0-beta.30"
  },
  "license": "MIT"
}
